chdir "{gerrit_working_dir}";
print "Pulling proposed changes into tree\n";
my $cmd = "git pull " . qw({gerrit_server}) . "/". qw({project}) . " refs/changes/" . 
   qw({changeid}) % 100 . "/" . qw({changeid}) ."/" . qw({patchid});
print "$cmd\n";
my $out = `$cmd 2>&1`;
my $ec = $?;
print "$out\n";
exit $ec;

