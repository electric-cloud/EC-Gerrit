<editor>
    <formElement>
        <type>entry</type>
        <label>Description:</label>
        <property>desc</property>
        <documentation>A description for this configuration</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Gerrit Server:</label>
        <property>gerrit_server</property>
        <documentation>The manifest file to use.</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>ElectricCommander Server:</label>
        <property>cmdr_webserver</property>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Project for Developer Builds:</label>
        <property>devbuild_cmdr_project</property>
        <documentation>The Commander project that containst the procedure for developer builds.</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Procedure for Developer Builds:</label>
        <property>devbuild_cmdr_procedure</property>
        <documentation>The Commander procedure that will be called for developer builds.</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Resource Pool</label>
        <property>ResourcePool</property>
        <documentation>Resource pool to use.</documentation>
    </formElement>
    <formElement>
        <type>select</type>
        <label>Developer Build Mode:</label>
        <property>devbuild_mode</property>
        <documentation>auto: Builds run automatically. manual: Builds must be initiated by clicking links in Gerrit. off: do not process this configuration for developer builds.</documentation>
        <option>
            <name>Auto</name>
            <value>auto</value>
        </option>
        <option>
            <name>Manual</name>
            <value>manual</value>
        </option>
        <option>
            <name>Off</name>
            <value>off</value>
        </option>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Branch:</label>
        <property>gerrit_branch</property>
        <documentation>The branch to watch.</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Manifest:</label>
        <property>gerrit_manifest</property>
        <documentation>The manifest file to use.</documentation>
    </formElement>
    <formElement>
        <type>textarea</type>
        <label>Team Build Filters:</label>
        <property>team_build_rules</property>
        <documentation>Rules for deciding which change sets to include in a team build.</documentation>
    </formElement>
    <formElement>
        <type>entry</type>
        <label>Debug:</label>
        <property>debug</property>
        <documentation>Debug level for output.</documentation>
    </formElement>
</editor>
